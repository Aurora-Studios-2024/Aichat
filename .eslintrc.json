{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eqeqeq": "off",
        "no-promise-executor-return": "off",
        "no-plusplus": "off",
        "no-console": "off",
        "quotes": ["error", "double"]
    }
}